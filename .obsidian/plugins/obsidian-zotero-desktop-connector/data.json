{
  "database": "Zotero",
  "noteImportFolder": "BEFORE/Inbox/",
  "pdfExportImageDPI": 120,
  "pdfExportImageFormat": "jpg",
  "pdfExportImageQuality": 90,
  "citeFormats": [
    {
      "name": "Cite",
      "format": "template",
      "template": "{{Citekey}}"
    }
  ],
  "exportFormats": [
    {
      "name": "Import Paper",
      "outputPathTemplate": "BEFORE/Inbox/{{citekey}}.md",
      "imageOutputPathTemplate": "BEFORE/Media/zotero/{{citekey}}/",
      "imageBaseNameTemplate": "{{citekey}}",
      "templatePath": "BEFORE/Templetes/Inputs/Paper.md"
    }
  ],
  "citeSuggestTemplate": "[[{{citekey}}]]",
  "openNoteAfterImport": true,
  "whichNotesToOpenAfterImport": "first-imported-note",
  "shouldConcat": true,
  "pdfExportImageOCR": true
}